{
  "expo": {
    "name": "artifact-impl-demo",
    "slug": "artifact-impl-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/upcycling.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.artifactimpldemo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/upcycling.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.artifactimpldemo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos so you can upload pictures of donated items."
        }
      ]
    ]
  }
}
